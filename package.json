{
    "name": "spotify-parser",
    "version": "1.0.6",
    "description": "Library for easy Spotify parsing without login",
    "keywords": [
        "spotify",
        "parser",
        "spotify-uri",
        "spotify-parser"
    ],
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "scripts": {
        "dev": "nodemon ./src/index.ts",
        "build": "rimraf dist && tsup",
        "build:publish": "pnpm run build && pnpm publish",
        "test": "jest"
    },
    "files": [
        "dist"
    ],
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.js"
        }
    },
    "author": "gulebi",
    "repository": {
        "type": "git",
        "url": "git://github.com/gulebi/spotify-parser.git"
    },
    "bugs": {
        "url": "https://github.com/gulebi/spotify-parser/issues"
    },
    "homepage": "https://github.com/gulebi/spotify-parser#readme",
    "license": "MIT",
    "dependencies": {
        "html5parser": "^2.0.2",
        "node-fetch": "^2.7.0",
        "spotify-uri": "^4.1.0"
    },
    "devDependencies": {
        "@types/jest": "^29.5.14",
        "@types/node": "^22.12.0",
        "@types/node-fetch": "^2.6.12",
        "jest": "^29.7.0",
        "nodemon": "^3.1.9",
        "rimraf": "^6.0.1",
        "ts-jest": "^29.2.5",
        "ts-node": "^10.9.2",
        "tsup": "^8.3.6",
        "typescript": "^5.7.3"
    },
    "packageManager": "pnpm@9.13.0"
}
